  "$schema": "../stryker-js/packages/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": ["html", "clear-text", "progress", "json"],
  "mutate": ["lib/numbers/basic.js"],
  "testRunner": "mocha",
  "coverageAnalysis": "perTest",
  "concurrency": 4,
  "timeoutMS": 2000,
  "mutator": {
    "excludedMutations": ["BlockStatement"]
